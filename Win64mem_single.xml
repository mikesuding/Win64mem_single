        <attribute>
            <name>scripttype</name>
            <value>powerShell</value>
            <comment></comment>
        </attribute>
        <attribute>
            <name>scriptgroovy</name>
            <value>$HostName = &#39;##SYSTEM.SYSNAME##&#39;
$UserName = &#39;##wmi.user##&#39;
$Password = &#39;##wmi.pass##&#39;
$CollectionScriptBlock = 
{
    $Win32OS = Get-WmiObject Win32_OperatingSystem | Select-Object FreePhysicalMemory,FreeSpaceInPagingFiles,FreeVirtualMemory,NumberOfProcesses,SizeStoredInPagingFiles,TotalVirtualMemorySize,TotalVisibleMemorySize
    $PerfData = Get-WmiObject Win32_PerfRawData_PerfOS_Memory | Select-Object AvailableBytes,CacheBytes,CacheFaultsPersec,DemandZeroFaultsPersec,FreeSystemPageTableEntries,PageReadsPersec,PageWritesPersec,PoolNonpagedBytes,PoolPagedBytes,TransitionFaultsPersec
    $Win32OS.PsObject.Properties | ForEach {Write-Host &#34;$($_.Name)=$($_.Value)&#34;}
    $PerfData.PsObject.Properties | ForEach {Write-Host &#34;$($_.Name)=$($_.Value)&#34;}
}
Try
{
    #If creds empty - invoke remote command without creds, if not - create credential object and use it for authorization
    if ($UserName -and $Password) {
        $Credentials = New-Object System.Management.Automation.PSCredential ($UserName, $(ConvertTo-SecureString $Password -AsPlainText -Force))
        Invoke-Command -ComputerName $HostName -ScriptBlock $CollectionScriptBlock -Credential $Credentials
    } else {
        Invoke-Command -ComputerName $HostName -ScriptBlock $CollectionScriptBlock
    }
}
Catch
{
    Exit 1
}
Exit 0</value>
